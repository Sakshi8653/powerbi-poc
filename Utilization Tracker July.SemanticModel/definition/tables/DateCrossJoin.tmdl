				    CROSSJOIN(DimFromMonth,DimToMonth,
				    SUMMARIZE('Time Period','Time Period'[MonthYearNumber]))
				    , 'Time Period'[MonthYearNumber] >= DimFromMonth[Month From Number] && 'Time Period'[MonthYearNumber] <= DimToMonth[Month To Number])

