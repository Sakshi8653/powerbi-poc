	column 'Total Hours (With Leaves)C 1' = ```
			
			VAR JoiningDate = 'Team'[Joining Date] -- Employee's Joining Date
			VAR ExitDate = 'Team'[Exit Date] -- Employee's Exit D 2ate
			
			-- Lookup the maximum End Date for all tries for the current employee
			VAR ValidEndDate = 
			    CALCULATE(
			        MAX('Time Entry'[End Date]),
			        FILTER(
			            ALL('Time Entry'), -- Removes any applied row context
			            'Time Entry'[User] = 'Team'[Employee]
			        )
			    )
			
			-- Adjust Start and End Dates to the valid range
			VAR StartDate = MAX(JoiningDate, MINX(FILTER(ALL('Time Entry'), 'Time Entry'[User] = 'Team'[Employee]), 'Time Entry'[Start Date]))
			VAR EndDate = MIN(ExitDate, ValidEndDate)
			
			-- Validate the date range
			VAR IsValidPeriod = NOT(ISBLANK(StartDate)) && NOT(ISBLANK(EndDate)) && StartDate <= EndDate
			
			-- Calculate the total hours for the entire valid period
			VAR TotalHours = 
			IF(
			    IsValidPeriod,
			    CALCULATE(
			        SUM('Time Period'[Hours in Month]),
			        FILTER(
			            ALL('Time Entry'),
			            'Time Entry'[Start Date] >= StartDate &&
			            'Time Entry'[End Date] <= EndDate &&
			            'Time Entry'[User] = 'Team'[Employee]
			        )
			    ),
			    0
			)
			
			-- Return the aggregated result
			RETURN
			IF(
			    IsValidPeriod,
			    TotalHours,
			    0 -- Return 0 for invalid periods
			)
			
			```
		formatString: 0
		lineageTag: c56ce193-f733-4cd0-95da-1314fe811989
		summarizeBy: sum

